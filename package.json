{
  "name": "armature",
  "version": "1.0.12",
  "description": "Typescript React components for MPLSART product suite",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "jest --collectCoverage true",
    "unit": "jest --testRegex '(?<!integration\\.)test\\.(t|j)s(x?)$'",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "dev-watch": "tsc --watch",
    "publishOnly": "npm run build",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://blainegarrett@github.com/mplsart/armature-2.git"
  },
  "author": "Blaine Garrett <blaine@mplsart.com>",
  "license": "ISC",
  "peerDependencies": {
    "@material-ui/core": "*",
    "@material-ui/icons": "*",
    "@material-ui/styles": "*",
    "moment-timezone": "*",
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "clsx": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@storybook/react": "^5.3.19",
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.24",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.1.0",
    "moment-timezone": "^0.5.31",
    "prettier": "^2.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "storybook": "^5.3.19",
    "storybook-addon-material-ui": "^0.9.0-alpha.21",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".test.(j|t)s(x?)$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.{ts,tsx}"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "jsdom",
    "resetMocks": true
  }
}
